CREATE TABLE preguntas (
    _id INTEGER AUTO INCREMENT PRIMARY KEY,
    question VARCHAR(256),
    correctAnswer VARCHAR(32) NOT NULL,
    wrongAnswer1 VARCHAR(32) NOT NULL,
    wrongAnswer2 VARCHAR(32) NOT NULL,
    wrongAnswer3 VARCHAR(32) NOT NULL,
    type CHARACTER NOT NULL DEFAULT 'B',
    res VARCHAR(100) DEFAULT NULL,
    resType CHARACTER NOT NULL DEFAULT 'N'
);